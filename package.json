{
  "name": "react-webpack-starter-kit",
  "version": "1.0.0",
  "description": "Sample reactJS project with redux, bootstrap and webpack.",
  "main": "dist/react-webpack-starter-kit.js",
  "author": "Micha≈Ç Psota",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start-prod || npm run start-dev",
    "start-dev": "webpack-dev-server --progress --inline --host 0.0.0.0 --colors --history-api-fallback",
    "start-prod": "webpack && node ./js/server.js",
    "hot-dev-server": "webpack-dev-server --hot --progress --colors --inline --host 0.0.0.0 --history-api-fallback",
    "build": "webpack --progress --profile --colors"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^5.8.25",
    "bootstrap": "^3.3.6",
    "bootstrap-slider": "6.1.1",
    "bootstrap-webpack": "^0.0.4",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.1",
    "imports-loader": "^0.6.3",
    "jquery": "~2.1.1",
    "json-loader": "^0.5.4",
    "less": "^1.7.5",
    "less-loader": "^0.7.7",
    "marked": "^0.3.2",
    "plotly.js": "^1.6.0",
    "react-plotlyjs": "^0.2.1",
    "react-router": "^2.3.0",
    "react-router-bootstrap": "^0.20.1",
    "react-treeview": "0.4.3",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-slider-monitor": "^1.0.5",
    "s-expression": "3.0.3",
    "style-loader": "^0.8.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.5"
  },
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "if-env": "^1.0.0",
    "immutable": "^3.8.1",
    "plotly.js": "^1.6.0",
    "react": "^0.14.7",
    "react-bootstrap": "^0.29.0",
    "react-bootstrap-combobox": "^3.0.1",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.5",
